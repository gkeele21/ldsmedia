puted: {\n    page() {\n      return parseInt(this.$route.query.page) || 1\n    },\n    hasNextPage() {\n      return this.eventsTotal > this.page * this.perPage\n    },\n    ...mapState(['events', 'eventsTotal', 'user'])\n  }\n}\n</script>\n"]}]}