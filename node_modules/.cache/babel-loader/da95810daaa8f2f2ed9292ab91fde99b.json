d,CAAf;AAMAa,MAAM,CAACM,UAAP,CAAkB,UAACb,OAAD,EAAUC,SAAV,EAAqBC,IAArB,EAA8B;AAC9CX,EAAAA,SAAS,CAACuB,KAAV;AACAZ,EAAAA,IAAI;AACL,CAHD;AAKAK,MAAM,CAACQ,SAAP,CAAiB,YAAM;AACrBxB,EAAAA,SAAS,CAACyB,IAAV;AACD,CAFD;AAGA,eAAeT,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport EventCreate from '../views/EventCreate.vue'\nimport EventList from '../views/EventList.vue'\nimport EventShow from '../views/EventShow.vue'\nimport NProgress from 'nprogress'\nimport store from '@/store/store'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'event-list',\n    component: EventList\n  },\n  {\n    path: '/event/:id',\n    name: 'event-show',\n    component: EventShow,\n    props: true,\n    beforeEnter(routeTo, routeFrom, next) {\n      store.dispatch('event/fetchEvent', routeTo.params.id).then(() => {\n        next()\n      })\n    }\n  },\n  {\n    path: '/event/create',\n    name: 'event-create',\n    component: EventCreate\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes\n})\n\nrouter.beforeEach((routeTo, routeFrom, next) => {\n  NProgress.start()\n  next()\n})\n\nrouter.afterEach(() => {\n  NProgress.done()\n})\nexport default router\n"]}]}