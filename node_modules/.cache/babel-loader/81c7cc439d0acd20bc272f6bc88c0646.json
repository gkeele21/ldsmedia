ACFW,QADH,CACY,kBADZ,EACgCH,OAAO,CAACI,MAAR,CAAeC,EAD/C,EAEGC,IAFH,CAEQ,UAAAC,KAAK,EAAI;AACbP,MAAAA,OAAO,CAACI,MAAR,CAAeG,KAAf,GAAuBA,KAAvB;AACAL,MAAAA,IAAI;AACL,KALH,EAMGM,KANH,CAMS,UAAAC,KAAK,EAAI;AACd,UAAIA,KAAK,CAACC,QAAN,IAAkBD,KAAK,CAACC,QAAN,CAAeC,MAAf,IAAyB,GAA/C,EAAoD;AAClDT,QAAAA,IAAI,CAAC;AAAEN,UAAAA,IAAI,EAAE,KAAR;AAAeQ,UAAAA,MAAM,EAAE;AAAEQ,YAAAA,QAAQ,EAAE;AAAZ;AAAvB,SAAD,CAAJ;AACD,OAFD,MAEO;AACLV,QAAAA,IAAI,CAAC;AAAEN,UAAAA,IAAI,EAAE;AAAR,SAAD,CAAJ;AACD;AACF,KAZH;AAaD;AAnBH,CAhBa,EAqCb;AACED,EAAAA,IAAI,EAAE,MADR;AAEEC,EAAAA,IAAI,EAAE,KAFR;AAGEC,EAAAA,SAAS,EAAET,QAHb;AAIEU,EAAAA,KAAK,EAAE;AAJT,CArCa,EA2Cb;AACEH,EAAAA,IAAI,EAAE,gBADR;AAEEC,EAAAA,IAAI,EAAE,eAFR;AAGEC,EAAAA,SAAS,EAAER;AAHb,CA3Ca,EAgDb;AACEM,EAAAA,IAAI,EAAE,GADR;AAEEkB,EAAAA,QAAQ,EAAE;AAAEjB,IAAAA,IAAI,EAAE,KAAR;AAAeQ,IAAAA,MAAM,EAAE;AAAEQ,MAAAA,QAAQ,EAAE;AAAZ;AAAvB;AAFZ,CAhDa,CAAf;AAsDA,IAAME,MAAM,GAAG,IAAI9B,SAAJ,CAAc;AAC3B+B,EAAAA,IAAI,EAAE,SADqB;AAE3BC,EAAAA,IAAI,EAAEC,OAAO,CAACC,GAAR,CAAYC,QAFS;AAG3BzB,EAAAA,MAAM,EAANA;AAH2B,CAAd,CAAf;AAMAoB,MAAM,CAACM,UAAP,CAAkB,UAACpB,OAAD,EAAUC,SAAV,EAAqBC,IAArB,EAA8B;AAC9CX,EAAAA,SAAS,CAAC8B,KAAV;AACAnB,EAAAA,IAAI;AACL,CAHD;AAKAY,MAAM,CAACQ,SAAP,CAAiB,YAAM;AACrB/B,EAAAA,SAAS,CAACgC,IAAV;AACD,CAFD;AAGA,eAAeT,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport EventCreate from '../views/EventCreate.vue'\nimport EventList from '../views/EventList.vue'\nimport EventShow from '../views/EventShow.vue'\nimport NotFound from '../views/NotFound.vue'\nimport NetworkIssue from '../views/NetworkIssue.vue'\nimport Example from '../views/Example.vue'\nimport NProgress from 'nprogress'\nimport store from '@/store/store'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'event-list',\n    component: EventList,\n    props: true\n  },\n  {\n    path: '/example',\n    component: Example\n  },\n  {\n    path: '/event/create',\n    name: 'event-create',\n    component: EventCreate\n  },\n  {\n    path: '/event/:id',\n    name: 'event-show',\n    component: EventShow,\n    props: true,\n    beforeEnter(routeTo, routeFrom, next) {\n      store\n        .dispatch('event/fetchEvent', routeTo.params.id)\n        .then(event => {\n          routeTo.params.event = event\n          next()\n        })\n        .catch(error => {\n          if (error.response && error.response.status == 404) {\n            next({ name: '404', params: { resource: 'event' } })\n          } else {\n            next({ name: 'network-issue' })\n          }\n        })\n    }\n  },\n  {\n    path: '/404',\n    name: '404',\n    component: NotFound,\n    props: true\n  },\n  {\n    path: '/network-issue',\n    name: 'network-issue',\n    component: NetworkIssue\n  },\n  {\n    path: '*',\n    redirect: { name: '404', params: { resource: 'page' } }\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes\n})\n\nrouter.beforeEach((routeTo, routeFrom, next) => {\n  NProgress.start()\n  next()\n})\n\nrouter.afterEach(() => {\n  NProgress.done()\n})\nexport default router\n"]}]}